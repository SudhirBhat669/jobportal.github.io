{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "run.py",
                "FLASK_ENV": "development"
            },
            "args": ["run", "--no-debugger"],
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Python: Flask Migrate",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "args": ["db", "migrate"],
            "console": "integratedTerminal"
        }
    ]
}

























































